{
  "name": "redisn",
  "version": "0.0.0",
  "description": "The next generation of redis client for Node.js. A modern, ultra performant and feature rich implementation.",
  "license": "APACHE-2.0",
  "repository": "github:Salakar/redisn",
  "author": {
    "name": "Mike Diarmid",
    "email": "mike.diarmid@gmail.com",
    "url": "http://github.com/Salakar/"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "flow": "flow",
    "lint": "eslint src",
    "prepublish": "babel src --out-dir lib",
    "build-watch": "rm -rf lib/ && babel --watch src --out-dir lib",
    "jest": "jest src",
    "test": "npm run flow && npm run lint && npm run jest",
    "format": "eslint --fix src"
  },
  "dependencies": {
    "cluster-key-slot": "^1.0.12",
    "denque": "^1.2.6",
    "redis-commands": "^1.3.5",
    "redis-parser": "^3.0.0",
    "redis-writable": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.73.0",
    "jest": "^23.0.1",
    "npm-release": "^1.0.0",
    "prettier": "^1.13.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 8
          }
        }
      ]
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier",
      "prettier/flowtype",
      "prettier/react"
    ],
    "plugins": [
      "flowtype",
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "globals": {
      "ReactClass": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "all",
          "singleQuote": true
        }
      ],
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "no-unused-expressions": 0,
      "new-cap": 0,
      "no-plusplus": 0,
      "no-class-assign": 0,
      "no-duplicate-imports": 0,
      "react/jsx-filename-extension": [
        0,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/sort-comp": 0,
      "react/forbid-prop-types": 1,
      "react/prop-types": 0,
      "react/require-default-props": 0,
      "import/no-duplicates": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "import/no-dynamic-require": 0,
      "flowtype/boolean-style": [
        2,
        "boolean"
      ],
      "flowtype/no-weak-types": 1,
      "flowtype/require-parameter-type": 2,
      "flowtype/require-return-type": [
        0,
        "always",
        {
          "annotateUndefined": "never"
        }
      ],
      "flowtype/require-valid-file-annotation": 2,
      "flowtype/use-flow-type": 1,
      "flowtype/valid-syntax": 1
    }
  }
}
